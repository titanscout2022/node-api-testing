{
  "name": "red-alliance-api",
  "version": "0.8.1",
  "description": "FRC 2022 Scouting API",
  "main": "index.js",
  "repository": "https://github.com/titanscout2022/red-alliance-api",
  "author": "Titan Scouting",
  "license": "BSD-3-Clause",
  "private": false,
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "start:prod": "npm run build && node built/index.js",
    "build": "tsc",
    "test-lint": "eslint . --ext .ts",
    "test": "nyc --reporter=text ts-mocha --timeout 10000 --exit",
    "coverage": "npm test && nyc report --reporter=lcov"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "test/**.js",
      "src/**.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@babel/register": "^7.10.5",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.26",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "google-auth-library": "^6.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.1",
    "mongo-express-req": "^3.0.1",
    "mongodb": "^3.5.9",
    "nodemon": "^2.0.2",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0",
    "uuid-apikey": "^1.4.6",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "minimist": ">=1.2.3",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "validator": "^13.1.1"
  }
}
