{
  "name": "red-alliance-api",
  "version": "0.9.0",
  "description": "FRC 2022 Scouting API",
  "main": "index.js",
  "repository": "https://github.com/titanscout2022/red-alliance-api",
  "author": "Titan Scouting",
  "license": "BSD-3-Clause",
  "private": false,
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "start:prod": "npm run build && node built/index.js",
    "build": "tsc",
    "test-lint": "eslint . --ext .ts",
    "test": "nyc --reporter=text ts-mocha --timeout 10000 --exit",
    "coverage": "npm test && nyc report --reporter=lcov",
    "doc": "typedoc --out docs src/ --exclude src/routes/ --exclude src/routes/index.ts --name 'The Red Alliance API' --includeVersion --hideGenerator"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "test/**.js",
      "src/**.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@babel/register": "^7.12.13",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.28",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validation": "^3.0.6",
    "google-auth-library": "^6.1.6",
    "hbs": "^4.1.1",
    "jsdoc": "^3.6.6",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.11.0",
    "mongo-express-req": "^3.0.1",
    "mongodb": "^3.6.4",
    "nodemon": "^2.0.5",
    "swagger-jsdoc": "^6.0.2",
    "swagger-ui-express": "^4.1.6",
    "tsdoc": "^0.0.4",
    "typescript": "^3.9.9",
    "uuid": "^8.3.2",
    "uuid-apikey": "^1.5.1",
    "validator": "^13.5.2",
    "snyk": "^1.454.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "minimist": ">=1.2.3",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "typedoc": "^0.20.16",
    "validator": "^13.5.2"
  },
  "snyk": true
}
