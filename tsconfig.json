{
    "compilerOptions": {
        "outDir": "./built",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "target": "es5",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": [
        "./src/**/*"
    ]
}   