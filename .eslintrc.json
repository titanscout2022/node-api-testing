{
    "extends": "airbnb",
    "rules": {
        "no-console": "off",
        "max-len": "off",
        "no-restricted-syntax": "off",
        "camelcase": "off"
    },
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"],
            "moduleDirectory": ["node_modules", "src/"]
          }
        }
      }
}